<%= form_tag '/recipes', method: :post do %>
<div>
  <%= label_tag :title %>
  <%= text_field_tag :title %>
</div>
<div>
  <%= label_tag :chef %>
  <%= text_field_tag :chef %>
</div>
<div>
  <%= label_tag :image %>
  <%= text_field_tag :image %>
</div>
<div>
  <%= submit_tag "Submit Recipe" %>
</div>


<% end %>